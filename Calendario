#include<iostream>
using namespace std;
int main (){
	float num;
	cout<<"Ingrese un numero: " ;
	cin>>num;
	if (num==1){
		cout<<"El numero " <<num<<" es Enero \n";
		}
		else{
	if (num==2){
		cout<<"El numero " <<num<<" es Febrero \n";
		}
	else{
			if (num==3){
		cout<<"El numero " <<num<<" es Marzo \n";
		}
	else{
		}
	if (num==4){
		cout<<"El numero " <<num<<" es Abril \n";
		}
	else{
	if (num==5){
		cout<<"El numero " <<num<<" es Mayo \n";
		}
	}
		}
		if (num==6){
		cout<<"El numero " <<num<<" es Junio \n";
		}
		else{
	if (num==7){
		cout<<"El numero " <<num<<" es Julio \n";
		}
	else{
			if (num==8){
		cout<<"El numero " <<num<<" es Agosto \n";
		}
	else{
		}
	if (num==9){
		cout<<"El numero " <<num<<" es Septiembre \n";
		}
	else{
	if (num==10){
		cout<<"El numero " <<num<<" es Octubre \n";
		}
		else{
	if (num==11){
		cout<<"El numero " <<num<<" es Noviembre \n";
		}else{
	if (num==12){
		cout<<"El numero " <<num<<" es Diciembre \n";
		}
	}
		}
		}
}
}
}
return 0;
}
